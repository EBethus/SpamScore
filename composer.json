    "description": "A Laravel lib to help detecting spam",
    "license": "MIT",
    "require": {
        "illuminate/support": "^8.0|^7.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "EBethus\\SpamScore\\": "src/SpamScore"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EBethus\\SpamScore\\SpamScoreServiceProvider"
            ],
            "aliases": {
                "SpamScore": "EBethus\\SpamScore\\SpamScoreFacade"
            }
        }
    }
}